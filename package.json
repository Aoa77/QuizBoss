{
    "name": "quizboss",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.105",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "copyfiles": "^2.4.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-scripts": "5.0.1",
        "typescript": "^4.9.5",
        "web-vitals": "^2.1.4"
    },
    "scripts": {

        "hello": "node scripts/_hello.js",
        "hellotest": "npm run hello -- --name='TEST 123!'",
        
        "find": "node scripts/findQuizModules.js",
        "findlocal": "npm run find -- --src=local_modules",
        "findnode": "npm run find -- --src=node_modules",

        "build": "npm run clean && npm run update && react-scripts build",
        "clean": "node scripts/cleanQuizModules.js",
        "update": "node scripts/updateQuizModules.js",
        "start": "npm run clean && npm run update && react-scripts start"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "minimalist": "^1.0.0"
    },
    "type": "module"
}
